root = true

[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2
charset = utf-8
trim_trailing_whitespace = true

[*.{sln,fsproj,csproj,fs,fsi,fsx,fs,csx,html,css,js}]
end_of_line = crlf

[*.{fs,fsi,fsx,cs,csx}]
indent_size = 3

[*.md]
trim_trailing_whitespace = false
